{
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "https://infamouspr.com/",
        "type": 301
      },
      {
        "source": "/**",
        "destination": "https://infamouspr.com/",
        "type": 301
      }
    ]
  },
  "storage": {
    "rules": "storage.json"
  }
}
